{
  "name": "eflow-api",
  "version": "1.0.0",
  "description": "eflow",
  "main": "index.js",
  "author": "Leo Qiu",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 6.1.0"
  },
  "scripts": {
    "build": "babel src -d dist",
    "serve": "node dist/index.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "start": "redis-cli flushall ; nodemon ./src/index.js --exec babel-node",
    "production": "redis-cli flushall ; NODE_ENV=production nodemon ./src/index.js --exec babel-node",
    "mocha": "./node_modules/.bin/_mocha test/service --exit --require babel-register --require babel-polyfill",
    "test": "NODE_ENV=test npm run test-prepare ; NODE_ENV=test npm run mocha",
    "test-cover": "NODE_ENV=test npm run test-prepare ; NODE_ENV=test nyc --reporter=text --reporter=lcov npm run mocha",
    "test-coverage": "nyc report --reporter=lcov",
    "migrate": "sequelize db:migrate",
    "prod-migrate": "NODE_ENV=production npm run migrate;",
    "seed": "sequelize db:seed:all",
    "prod-seed": "NODE_ENV=production sequelize db:seed:all",
    "prod:prepare": "NODE_ENV=production sequelize db:migrate:undo:all ; NODE_ENV=production npm run migrate ; NODE_ENV=production npm run seed",
    "test:prepare": "sequelize db:migrate:undo:all ; npm run migrate",
    "dev:prepare": "sequelize db:migrate:undo:all ; npm run migrate ; npm run seed",
    "lint": "node_modules/.bin/eslint --fix --ext \".js\" --ignore-pattern node_modules --quiet .",
    "prettier": "prettier --single-quote --bracket-spacing false --trailing-comma es5 --write '{src,__{tests,mocks}__}/**/*.js'",
    "pm2:production": "NODE_ENV=production pm2 start ecosystem.json --env production",
    "pm2:restart": "npm run build && pm2 restart eflow-api",
    "pm2:logs": "pm2 logs"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "aws-sdk": "^2.292.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.2",
    "chai": "^4.0.2",
    "chai-http": "^4.2.1",
    "cors": "^2.8.4",
    "coveralls": "^3.0.0",
    "cron": "^1.3.0",
    "csvtojson": "^1.1.9",
    "d3": "^5.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-plugin-prettier": "^3.0.1",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "factory-girl": "^5.0.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.5",
    "mocha": "^4.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.0",
    "nodemailer": "^6.4.16",
    "nodemailer-mailgun-transport": "^1.3.6",
    "nodemon": "^1.19.0",
    "nyc": "^14.1.1",
    "pg": "^6.4.1",
    "pg-hstore": "^2.3.2",
    "pm2": "^3.0.3",
    "prettier": "1.16.4",
    "redis": "^2.8.0",
    "request": "^2.83.0",
    "sequelize": "^4.33.4",
    "sequelize-cli": "^5.4.0",
    "socket.io": "^2.1.0",
    "superagent": "^3.8.3",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {}
}
